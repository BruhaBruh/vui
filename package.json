{
  "name": "@bruhabruh/vui",
  "version": "0.0.4",
  "type": "module",
  "description": "BruhaBruh UI Library",
  "author": "BruhaBruh",
  "license": "MIT",
  "keywords": [],
  "repository": {
    "type": "git",
    "url": "https://github.com/bruhabruh/vui"
  },
  "bin": "./dist/index.js",
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check \"build-only {@}\" --",
    "build:cli": "tsup",
    "preview": "vite preview",
    "build-only": "vite build",
    "type-check": "vue-tsc --build",
    "lint": "eslint . --fix",
    "format": "prettier --write src/",
    "cli": "tsx ./src/cli/client/index.ts",
    "generate:theme": "pnpm dlx @bruhabruh/theme@latest generate",
    "generate:registry": "tsx ./src/cli/generate/index.ts registry",
    "generate:config-schema": "tsx ./src/cli/generate/index.ts config-schema",
    "generate:registry-schema": "tsx ./src/cli/generate/index.ts registry-schema"
  },
  "dependencies": {
    "@antfu/ni": "^24.3.0",
    "@floating-ui/vue": "^1.1.6",
    "@tabler/icons-vue": "^3.31.0",
    "@vueuse/core": "^13.1.0",
    "@vueuse/integrations": "^13.1.0",
    "class-variance-authority": "^0.7.1",
    "commander": "^13.1.0",
    "embla-carousel-vue": "^8.6.0",
    "focus-trap": "^7.6.4",
    "kleur": "^4.1.5",
    "motion-v": "1.0.1",
    "ora": "^8.2.0",
    "prompts": "^2.4.2",
    "shiki": "^3.3.0",
    "tailwind-merge-vue-directive": "^2.0.5",
    "vue": "^3.5.13",
    "vue-router": "^4.5.1",
    "zod": "4.0.0-beta.20250503T014749"
  },
  "devDependencies": {
    "@bruhabruh/eslint-config": "^1.3.3",
    "@bruhabruh/prettier-config": "^1.1.4",
    "@bruhabruh/type-safe": "^1.2.3",
    "@tailwindcss/vite": "^4.1.5",
    "@tsconfig/node22": "^22.0.0",
    "@types/node": "^22.15.3",
    "@types/prompts": "^2.4.9",
    "@vitejs/plugin-vue": "^5.2.1",
    "@vue/eslint-config-prettier": "^10.2.0",
    "@vue/eslint-config-typescript": "^14.5.0",
    "@vue/tsconfig": "^0.7.0",
    "@vue/typescript-plugin": "^2.2.10",
    "eslint": "^9.26.0",
    "eslint-plugin-vue": "~10.1.0",
    "jiti": "^2.4.2",
    "npm-run-all2": "^8.0.1",
    "prettier": "3.5.3",
    "tailwindcss": "^4.1.5",
    "tsup": "^8.4.0",
    "tsx": "^4.19.4",
    "typescript": "~5.8.3",
    "vite": "^6.3.4",
    "vite-plugin-vue-devtools": "^7.7.6",
    "vue-tsc": "^2.2.10"
  }
